{
  "name": "awa-helper",
  "description": "AWA自动任务",
  "version": "1.0.1",
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "author": {
    "email": "h1606051253@gmail.com",
    "name": "HCLonely"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "release": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=true",
    "package": "node scripts/package.js",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 npx --no-install electron ./scripts/update-electron-vendors.js "
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.0.0",
    "@types/tunnel": "^0.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "3.0.0-beta.0",
    "@vue/test-utils": "2.0.1",
    "cross-env": "7.0.3",
    "electron": "19.0.6",
    "electron-builder": "23.1.0",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.18.0",
    "eslint-plugin-vue": "9.1.1",
    "happy-dom": "5.3.2",
    "nano-staged": "0.8.0",
    "playwright": "1.22.2",
    "simple-git-hooks": "2.8.0",
    "typescript": "4.7.4",
    "unplugin-auto-expose": "0.0.1",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.20.1",
    "vite": "3.0.0-beta.3",
    "vite-plugin-electron-renderer": "^0.5.0",
    "vitest": "0.16.0",
    "vue-tsc": "0.38.2",
    "zip-dir": "^2.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.11.3",
    "electron-updater": "5.0.5",
    "element-plus": "^2.2.6",
    "form-data": "^4.0.0",
    "path-browserify": "^1.0.1",
    "qs": "^6.11.0",
    "socks-proxy-agent": "^7.0.0",
    "tunnel": "^0.0.6",
    "uuid": "^8.3.2",
    "vue": "3.2.37",
    "yaml": "^2.1.1",
    "yaml-lint": "^1.7.0"
  }
}
